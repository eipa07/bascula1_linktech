<mvc:View
    controllerName="zqmed03bascula.zqmed03bascula.controller.Detail"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:tnt="sap.tnt"
    xmlns:table="sap.ui.table"
    xmlns:l="sap.ui.layout"
>
      <tnt:ToolHeader>
      <Button text="{i18n>detailview.regresar}" press="goBackBtn"/>
        <ToolbarSpacer />
        <Title
            titleStyle="H4"
            text="{i18n>title}"
            class="sapUiSmallMargin"
            id="titleText"
        />
        <ToolbarSpacer />
    </tnt:ToolHeader>
    <Page
        id="page"
        title="{i18n>title}"
        showHeader="false"
    >
        <content>
            

            

            <!--Detail table-->

            <table:Table
                id="MainView_Table_Detail"
                rows="{detailModel>/}"
                selectionMode="MultiToggle"
                paste="onPaste"
                ariaLabelledBy="title"
                enableCellFilter="true"
                rowMode="Fixed"
            >
                <table:columns>
                    <table:Column
                        filterProperty="Sec"
                        sortProperty="Sec"
                        id="MainView_Column01_Detail"
                        width="auto"
                    >
                        <Label
                            id="MainView_Label01_Detail"
                            text="{i18n>mainview.detail.sec}"
                        />
                        <table:template>
                            <Text
                                id="MainView_Text01_Detail"
                                text="{detailModel>Sec}"
                                wrapping="true"
                            />
                        </table:template>
                    </table:Column>

                    <table:Column
                        filterProperty="Producto"
                        sortProperty="Producto"
                        id="MainView_Column02_Detail"
                        width="auto"
                    >
                        <Label
                            id="MainView_Label02_Detail"
                            text="{i18n>mainview.detail.Producto}"
                        />
                        <table:template>
                            <Text
                                id="MainView_Text02_Detail"
                                text="{detailModel>Producto}"
                                wrapping="true"
                            />
                        </table:template>
                    </table:Column>

                    <table:Column
                        filterProperty="Descripcion"
                        sortProperty="Descripcion"
                        id="MainView_Column03_Detail"
                        width="auto"
                    >
                        <Label
                            id="MainView_Label03_Detail"
                            text="{i18n>mainview.detail.Descripcion}"
                        />
                        <table:template>
                            <Text
                                id="MainView_Text03_Detail"
                                text="{detailModel>Descripcion}"
                                wrapping="true"
                            />
                        </table:template>
                    </table:Column>

                    <table:Column
                        filterProperty="Presentacion"
                        sortProperty="Presentacion"
                        id="MainView_Column04_Detail"
                        width="auto"
                    >
                        <Label
                            id="MainView_Label04_Detail"
                            text="{i18n>mainview.detail.Presentacion}"
                        />
                        <table:template>
                            <Text
                                id="MainView_Text04_Detail"
                                text="{detailModel>Presentacion}"
                                wrapping="true"
                            />
                        </table:template>
                    </table:Column>

                    <table:Column
                        filterProperty="Pacas"
                        sortProperty="Pacas"
                        id="MainView_Column05_Detail"
                        width="auto"
                    >
                        <Label
                            id="MainView_Label05_Detail"
                            text="{i18n>mainview.detail.Pacas}"
                        />
                        <table:template>
                            <Text
                                id="MainView_Text05_Detail"
                                text="{detailModel>Pacas}"
                                wrapping="true"
                            />
                        </table:template>
                    </table:Column>

                    <table:Column
                        filterProperty="kg_Bruto"
                        sortProperty="kg_Bruto"
                        id="MainView_Column06_Detail"
                        width="auto"
                    >
                        <Label
                            id="MainView_Label06_Detail"
                            text="{i18n>mainview.detail.kg_Bruto}"
                        />
                        <table:template>
                            <Text
                                id="MainView_Text06_Detail"
                                text="{detailModel>kg_Bruto}"
                                wrapping="true"
                            />
                        </table:template>
                    </table:Column>

                    <table:Column
                        filterProperty="Kg_Tara"
                        sortProperty="Kg_Tara"
                        id="MainView_Column07_Detail"
                        width="auto"
                    >
                        <Label
                            id="MainView_Label07_Detail"
                            text="{i18n>mainview.detail.Kg_Tara}"
                        />
                        <table:template>
                            <Text
                                id="MainView_Text07_Detail"
                                text="{detailModel>Kg_Tara}"
                                wrapping="true"
                            />
                        </table:template>
                    </table:Column>

                    <table:Column
                        filterProperty="Kg_Tara_Adic"
                        sortProperty="Kg_Tara_Adic"
                        id="MainView_Column08_Detail"
                        width="auto"
                    >
                        <Label
                            id="MainView_Label08_Detail"
                            text="{i18n>mainview.detail.Kg_Tara_Adic}"
                        />
                        <table:template>
                            <Text
                                id="MainView_Text08_Detail"
                                text="{detailModel>Kg_Tara_Adic}"
                                wrapping="true"
                            />
                        </table:template>
                    </table:Column>

                    <table:Column
                        filterProperty="Pct_Hum"
                        sortProperty="Pct_Hum"
                        id="MainView_Column09_Detail"
                        width="auto"
                    >
                        <Label
                            id="MainView_Label09_Detail"
                            text="{i18n>mainview.detail.Pct_Hum}"
                        />
                        <table:template>
                            <Text
                                id="MainView_Text09_Detail"
                                text="{detailModel>Pct_Hum}"
                                wrapping="true"
                            />
                        </table:template>
                    </table:Column>

                    <table:Column
                        filterProperty="Kg_Hum"
                        sortProperty="Kg_Hum"
                        id="MainView_Column10_Detail"
                        width="auto"
                    >
                        <Label
                            id="MainView_Label10_Detail"
                            text="{i18n>mainview.detail.Kg_Hum}"
                        />
                        <table:template>
                            <Text
                                id="MainView_Text10_Detail"
                                text="{detailModel>Kg_Hum}"
                                wrapping="true"
                            />
                        </table:template>
                    </table:Column>

                    <table:Column
                        filterProperty="Pct_Imp"
                        sortProperty="Pct_Imp"
                        id="MainView_Column11_Detail"
                        width="auto"
                    >
                        <Label
                            id="MainView_Label11_Detail"
                            text="{i18n>mainview.detail.Pct_Imp}"
                        />
                        <table:template>
                            <Text
                                id="MainView_Text11_Detail"
                                text="{detailModel>Pct_Imp}"
                                wrapping="true"
                            />
                        </table:template>
                    </table:Column>

                    <table:Column
                        filterProperty="Kg_Imp"
                        sortProperty="Kg_Imp"
                        id="MainView_Column12_Detail"
                        width="auto"
                    >
                        <Label
                            id="MainView_Label12_Detail"
                            text="{i18n>mainview.detail.Kg_Imp}"
                        />
                        <table:template>
                            <Text
                                id="MainView_Text12_Detail"
                                text="{detailModel>Kg_Imp}"
                                wrapping="true"
                            />
                        </table:template>
                    </table:Column>

                    <table:Column
                        filterProperty="Kg_Neto_Park"
                        sortProperty="Kg_Neto_Park"
                        id="MainView_Column13_Detail"
                        width="auto"
                    >
                        <Label
                            id="MainView_Label13_Detail"
                            text="{i18n>mainview.detail.Kg_Neto_Park}"
                        />
                        <table:template>
                            <Text
                                id="MainView_Text13_Detail"
                                text="{detailModel>Kg_Neto_Park}"
                                wrapping="true"
                            />
                        </table:template>
                    </table:Column>

                    <table:Column
                        filterProperty="Precio"
                        sortProperty="Precio"
                        id="MainView_Column14_Detail"
                        width="auto"
                    >
                        <Label
                            id="MainView_Label14_Detail"
                            text="{i18n>mainview.detail.Precio}"
                        />
                        <table:template>
                            <Text
                                id="MainView_Text14_Detail"
                                text="{detailModel>Precio}"
                                wrapping="true"
                            />
                        </table:template>
                    </table:Column>

                    <table:Column
                        filterProperty="Valor"
                        sortProperty="Valor"
                        id="MainView_Column15_Detail"
                        width="auto"
                    >
                        <Label
                            id="MainView_Label15_Detail"
                            text="{i18n>mainview.detail.Valor}"
                        />
                        <table:template>
                            <Text
                                id="MainView_Text15_Detail"
                                text="{detailModel>Valor}"
                                wrapping="true"
                            />
                        </table:template>
                    </table:Column>
                </table:columns>
                <table:footer>
                    <HBox>
                        <ToolbarSpacer width="3rem" />
                        <Button
                            type="Ghost"
                            text="Incluir"
                            press="onPressIncluir"
                            ariaDescribedBy="defaultButtonDescription genericButtonDescription"
                        >
                            <layoutData>
                                <FlexItemData growFactor="1" />
                            </layoutData>
                        </Button>
                        <Button
                            type="Ghost"
                            text="Modificar"
                            press="onPress"
                            ariaDescribedBy="acceptButtonDescription genericButtonDescription"
                        >
                            <layoutData>
                                <FlexItemData growFactor="1" />
                            </layoutData>
                        </Button>
                        <Button
                            type="Ghost"
                            text="Eliminar"
                            press="onPress"
                            ariaDescribedBy="rejectButtonDescription genericButtonDescription"
                        >
                            <layoutData>
                                <FlexItemData growFactor="1" />
                            </layoutData>
                        </Button>
                        <Text
                            text="{i18n>mainview.detail.txt01}"
                            
                            ariaDescribedBy="comingSoonButtonDescription genericButtonDescription"
                        >
                            <layoutData>
                                <FlexItemData growFactor="1" />
                            </layoutData>
                        </Text>
                        <Text text="{i18n>mainview.detail.txt02}" ariaDescribedBy="comingSoonButtonDescription genericButtonDescription" />
                        <Text text="" ariaDescribedBy="comingSoonButtonDescription genericButtonDescription"/>
                        <Label
                            text=""
                            ariaDescribedBy="comingSoonButtonDescription genericButtonDescription"
                        >
                            <layoutData>
                                <FlexItemData growFactor="1" />
                            </layoutData>
                        </Label>
                        <Text text="{i18n>mainview.detail.txt03}" ariaDescribedBy="comingSoonButtonDescription genericButtonDescription"/>
                        <Text text="" ariaDescribedBy="comingSoonButtonDescription genericButtonDescription"/>
                        
                    </HBox>
                </table:footer>
            </table:Table>
        </content>
        <footer>
			<OverflowToolbar>
				<ToolbarSpacer/>
				<Button type="Accept" text="Accept"/>
				<Button type="Reject" text="Reject"/>
			</OverflowToolbar>
		</footer>
    </Page>
</mvc:View>
